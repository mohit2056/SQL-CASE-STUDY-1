Task01.sql — Total sales

SELECT 
  SUM(F.Sales) AS TotalSales
FROM FactTable F;

Task02.sql — Total profit
SELECT 
  SUM(F.Profit) AS TotalProfit
FROM FactTable F;


Task03.sql — Total COGS
SELECT 
  SUM(F.COGS) AS TotalCOGS
FROM FactTable F;


Task04.sql — Average sales
SELECT 
  AVG(F.Sales) AS AvgSales
FROM FactTable F;


Task05.sql — Average profit
SELECT 
  AVG(F.Profit) AS AvgProfit
FROM FactTable F;


Task06.sql — Average COGS
SELECT 
  AVG(F.COGS) AS AvgCOGS
FROM FactTable F;


Task07.sql — Total sales by market
SELECT 
  L.Market,
  SUM(F.Sales) AS TotalSales
FROM FactTable F
JOIN LocationTable L 
  ON F.AreaCode = L.AreaCode
GROUP BY L.Market
ORDER BY TotalSales DESC;


Task08.sql — Total profit by market
